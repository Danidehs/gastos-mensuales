{
  "name": "gastos-mensuales",
  "version": "1.1.0",
  "description": "Package.json principal para app con funciones de llevar un registro de gastos",
  "private": true,
  "scripts": {
    "start": "vercel dev",
    "build": "cd client && npm run build",
    "deploy": "vercel --prod",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "danidehs",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.7.0",
    "mongoose": "^8.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "@vercel/node": "^1.8.0"
  }
}
